<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="520.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.LoginController">
   <children>
      <AnchorPane prefHeight="106.0" prefWidth="520.0" style="-fx-background-color: #88001b;">
         <children>
            <ImageView fx:id="loginImageView" fitHeight="65.0" fitWidth="265.0" layoutX="172.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@loginandlock.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Label layoutX="100.0" layoutY="123.0" prefHeight="26.0" prefWidth="66.0" text="Username:" textFill="#88001b" />
      <TextField fx:id="usernameField" layoutX="92.0" layoutY="149.0" prefHeight="25.0" prefWidth="336.0" promptText="Username">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <Label layoutX="100.0" layoutY="176.0" prefHeight="26.0" prefWidth="66.0" text="Password:" textFill="#88001b" />
      <Label layoutX="100.0" layoutY="228.0" prefHeight="26.0" prefWidth="66.0" text="Role:" textFill="#88001b" />
      <ChoiceBox fx:id="role" layoutX="92.0" layoutY="248.0" prefHeight="25.0" prefWidth="336.0" />
      <PasswordField fx:id="passwordField" layoutX="92.0" layoutY="202.0" prefHeight="25.0" prefWidth="336.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="92.0" layoutY="301.0" mnemonicParsing="false" onAction="#loginButtonOnAction" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #88001b;" text="Login" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="92.0" layoutY="344.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #88001b;" text="Back" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <ImageView fx:id="weightImageView" fitHeight="46.0" fitWidth="66.0" layoutX="14.0" layoutY="335.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@weight.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="gymappimageView" fitHeight="89.0" fitWidth="78.0" layoutX="434.0" layoutY="110.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@LOGOAPP.jpg" />
         </image>
      </ImageView>
      <Label fx:id="loginMessage" layoutX="45.0" layoutY="109.0" prefHeight="20.0" prefWidth="400.0" textFill="#88001b">
         <font>
            <Font size="13.0" />
         </font></Label>
   </children>
</AnchorPane>
